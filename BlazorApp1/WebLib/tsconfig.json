{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "ES6",
    "outDir": "../wwwroot/js",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "Component",
    "Pages",
    "Layout"
  ],
  "include": [
    "**/*"
  ]
}
